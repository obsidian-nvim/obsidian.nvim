{
  "runtime": {
    "version": "LuaJIT",
    "requirePattern": [
      "lua/?.lua",
      "lua/?/init.lua",
      "?/lua/?.lua",
      "?/lua/?/init.lua"
    ]
  },
  "workspace": {
    "checkThirdParty": false,
    "library": [
      "$VIMRUNTIME"
    ],
    "ignoreGlobs": [
      "tests/test_*.lua"
    ]
  }
}
